{
  "name": "flash-cards",
  "version": "0.0.1",
  "description": "A project for my EDPS 6560 multimedia learning class",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "scripts/build.sh",
    "deploy": "npm run build && node scripts/deploy.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "run-p --print-label test:*",
    "test:integration":
      "NODE_ENV=test start-server-and-test start http://localhost:1234 cypress:run",
    "test:unit": "jest",
    "test:lint": "eslint --no-inline-config \"{src, cypress}/**/*.js\"",
    "test:prettier": "prettier --list-different \"{src, cypress}/**/*.js\"",
    "precommit": "lint-staged"
  },
  "keywords": ["flash", "cards", "learning"],
  "author": "Clay Diffrient",
  "license": "MIT",
  "dependencies": {
    "@instructure/ui-core": "^4.7.0",
    "@instructure/ui-themes": "^4.7.0",
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "instructure-icons": "^4.3.2",
    "react": "^16.2.0",
    "react-card-flip": "^0.2.1",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-quill": "^1.2.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-offline": "^2.0.0",
    "redux-thunk": "^2.2.0",
    "xss": "^0.3.7"
  },
  "devDependencies": {
    "@instructure/babel-plugin-themeable-styles": "^4.7.0",
    "@instructure/babel-plugin-transform-class-display-name": "^4.7.0",
    "@instructure/postcss-themeable-styles": "^4.7.0",
    "autoprefixer": "^7.2.5",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cssnext": "^1.8.4",
    "cypress": "^2.0.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.5.1",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "lint-staged": "^7.0.0",
    "moment": "^2.20.1",
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "^1.6.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-modules": "^1.1.0",
    "pouchdb": "^6.4.3",
    "pouchdb-adapter-memory": "^6.4.3",
    "pouchdb-erase": "^1.0.2",
    "pouchdb-find": "^6.4.3",
    "prettier": "^1.10.2",
    "prop-types": "^15.6.0",
    "react-hot-loader": "^3.1.3",
    "redux-logger": "^3.0.6",
    "relational-pouch": "^3.0.1",
    "shelljs": "^0.8.1",
    "start-server-and-test": "^1.0.0"
  },
  "lint-staged": {
    "*.{json,css,md}": ["prettier --write", "git add"],
    "{src, cypress}/**/*.js": ["eslint --fix", "git add"]
  }
}
