{
  "name": "flash-cards",
  "version": "0.0.1",
  "description": "A project for my EDPS 6560 multimedia learning class",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "scripts/run.js build",
    "deploy": "scripts/run.js deploy",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "run-p --print-label test:*",
    "test:integration":
      "NODE_ENV=test start-server-and-test start http://localhost:1234 cypress:run",
    "test:unit": "jest",
    "test:lint":
      "eslint --no-inline-config \"{src, scripts, cypress}/**/*.js\"",
    "test:prettier":
      "prettier --list-different \"{src, scripts, cypress}/**/*.js\"",
    "precommit": "lint-staged"
  },
  "keywords": ["flash", "cards", "learning"],
  "author": "Clay Diffrient",
  "license": "MIT",
  "dependencies": {
    "@instructure/ui-billboard": "^5.2.0",
    "@instructure/ui-buttons": "^5.2.0",
    "@instructure/ui-container": "^5.2.0",
    "@instructure/ui-elements": "^5.2.0",
    "@instructure/ui-forms": "^5.2.0",
    "@instructure/ui-layout": "^5.2.0",
    "@instructure/ui-menu": "^5.2.0",
    "@instructure/ui-overlays": "^5.2.0",
    "@instructure/ui-themeable": "^5.2.0",
    "@instructure/ui-themes": "^5.2.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "instructure-icons": "^4.3.2",
    "raven-js": "^3.24.0",
    "react": "^16.3.1",
    "react-card-flip": "^0.2.1",
    "react-dom": "^16.3.1",
    "react-loadable": "^5.3.1",
    "react-quill": "^1.2.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-actions": "^2.3.0",
    "redux-offline": "^2.0.0",
    "redux-thunk": "^2.2.0",
    "xss": "^0.3.7"
  },
  "devDependencies": {
    "@instructure/babel-plugin-themeable-styles": "5.7.0",
    "@instructure/babel-plugin-transform-class-display-name": "5.7.0",
    "@instructure/postcss-themeable-styles": "5.7.0",
    "@instructure/ui-codemods": "5.7.0",
    "@sentry/cli": "1.31.0",
    "autoprefixer": "8.6.5",
    "babel-eslint": "8.2.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "core-js": "2.5.6",
    "cypress": "2.1.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.3",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-cypress": "2.0.1",
    "eslint-plugin-jest": "21.17.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "gh-pages": "1.1.0",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.1.0",
    "lint-staged": "7.1.0",
    "moment": "2.22.1",
    "npm-run-all": "4.1.3",
    "parcel-bundler": "1.8.1",
    "postcss-cssnext": "3.1.0",
    "postcss-modules": "1.1.0",
    "pouchdb": "6.4.3",
    "pouchdb-adapter-memory": "6.4.3",
    "pouchdb-erase": "1.0.2",
    "pouchdb-find": "6.4.3",
    "prettier": "1.12.1",
    "prop-types": "15.6.1",
    "react-hot-loader": "4.2.0",
    "redux-logger": "3.0.6",
    "relational-pouch": "3.0.1",
    "shelljs": "0.8.2",
    "start-server-and-test": "1.4.1"
  },
  "lint-staged": {
    "*.{json,css,md}": ["prettier --write", "git add"],
    "{src, cypress, scripts}/**/*.js": ["eslint --fix", "git add"]
  }
}
